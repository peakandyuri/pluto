{
	"compilerOptions": {
		"module": "esnext",
		"target": "esnext",
		"experimentalDecorators": true,
		"sourceMap": true,
		"allowSyntheticDefaultImports": true,
		"jsx": "react",
		"allowJs": false,
		"baseUrl": ".",
		"moduleResolution": "node",
		"paths": {
			"@/*": [
				"src/*"
			]
		},
	},
	"exclude": [
		"node_modules"
	]
}